        "babel-preset-stage-0": "^6.16.0",
        "babel-runtime": "^6.23.0",
        "better-npm-run": "0.0.14",
        "cli-color": "^1.2.0",
        "compression-webpack-plugin": "^0.3.2",
        "copy-webpack-plugin": "^4.0.1",
        "css-loader": "0.23.1",
